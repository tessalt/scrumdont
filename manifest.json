{
  "manifest_version": 2,
  "name": "ScrumDon't 2.0",
  "description": "Strollin'",
  "version": "2.2",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "scrumdont_19.png"
    }
  },
  "icons": {
    "48": "scrumdont_48.png",
    "128": "scrumdont_128.png"
  },
   "background": {
    "scripts": ["js/eventpage.js"]
  },
  "permissions":[
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "storage"
    ]
}
